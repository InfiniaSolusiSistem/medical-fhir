<odoo>

    <record id="wizard_state_postponed" model="medical.careplan.medical.wizard.state">
        <field name="name">Postponed</field>
    </record>

    <record id="wizard_state_done" model="medical.careplan.medical.wizard.state">
        <field name="name">Done</field>
        <field name="done" eval="True"/>
    </record>

    <record id="wizard_state_generate_next" model="medical.careplan.medical.wizard.state">
        <field name="name">Generate Next</field>
        <field name="only_timing" eval="True"/>
    </record>

</odoo>
